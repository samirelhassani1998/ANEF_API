{
  "manifest_version": 3,
  "name": "ANEF Dossier Status (Text Only)",
  "version": "1.0.0",
  "description": "Affiche en texte brut le statut et les dates principales du dossier de naturalisation ANEF.",
  "host_permissions": [
    "https://administration-etrangers-en-france.interieur.gouv.fr/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://administration-etrangers-en-france.interieur.gouv.fr/*"
      ],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
